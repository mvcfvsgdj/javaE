package day20.thread;

public class AutoSaveEx {

	public static void main(String[] args) {

		AutoSave s = new AutoSave();
		s.setDaemon(true);
		s.start();

	}
}

/////////////////////////////////////////
class AutoSave extends Thread {
	void autoSave() {
		System.out.println("NOT SAVE GO CRY");
	}
	@Override
	public void run() {

		while(true) {
			try {
				sleep(3000);
			} catch (InterruptedException e) {
	
				e.printStackTrace();
			}
			
		}
		
	}
}